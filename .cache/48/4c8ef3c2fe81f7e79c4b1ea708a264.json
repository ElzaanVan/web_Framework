"src/views/UserForm.ts","name":null,"original":{"line":3,"column":13},"generated":{"line":80,"column":8}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":3,"column":13},"generated":{"line":80,"column":16}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":3,"column":13},"generated":{"line":80,"column":19}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":3,"column":13},"generated":{"line":80,"column":27}}],"sources":{"src/views/UserForm.ts":"import { User } from '../models/User';\r\n\r\nexport class UserForm {\r\n    constructor(\r\n        public parent: Element,\r\n        public model: User\r\n    ) {\r\n        this.bindModel();\r\n    }\r\n\r\n    bindModel(): void {\r\n        this.model.on('change', () => {\r\n            this.render();\r\n        });\r\n    }\r\n\r\n    eventsMap(): { [key: string]: () => void } {\r\n        return {\r\n            'click:.set-age' : this.onsetAgeClick,\r\n            'click:.change-name' : this.onChangeNameClick\r\n        };\r\n    }\r\n\r\n    //Arrow function for lexical binding to prevent undefined\r\n    onsetAgeClick = (): void => {\r\n        this.model.setRandomAge();\r\n    }\r\n\r\n    onChangeNameClick = (): void => {\r\n        const input = this.parent.querySelector('input');\r\n\r\n        const name = input.value;\r\n        this.model.set({ name })\r\n    }\r\n\r\n    template(): string {\r\n        return `\r\n        <div>\r\n            <h1>User Form</h1>\r\n            <p>User name: ${this.model.get('name')}</p>\r\n            <p>User age: ${this.model.get('age')}</p>\r\n            <input />\r\n            <button class=\"change-name\">Change name</button>\r\n            <br />\r\n            <br />\r\n            <button class=\"set-age\">Set random age</button>\r\n        </div>\r\n        `;\r\n    }\r\n\r\n    bindEvents(fragment: DocumentFragment): void {\r\n        const eventsMap = this.eventsMap();\r\n\r\n        for (let eventKey in eventsMap) {\r\n            const [eventName, selector] = eventKey.split(':');\r\n\r\n            fragment.querySelectorAll(selector).forEach(el => {\r\n                el.addEventListener(eventName, eventsMap[eventKey]);\r\n            });\r\n        }\r\n    }\r\n\r\n    render(): void {\r\n        this.parent.innerHTML = '';\r\n        const templateElement = document.createElement('template');\r\n        templateElement.innerHTML = this.template();\r\n        this.bindEvents(templateElement.content);\r\n        this.parent.append(templateElement.content);\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"08dfdb6f62f15aac52999ab260921fcb","cacheData":{"env":{}}}